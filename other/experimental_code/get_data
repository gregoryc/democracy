window.scrollTo(0, 250); olds=[];old="";function f() { new_ = (q=[...document.querySelectorAll("text")]).at(-1).innerHTML; (1); if (new_ != old) { console.log(new_);/* } */        old = new_;         olds.push(JSON.stringify(old)+":"+JSON.stringify(q[3].innerHTML)+"<br>");   }; };       setInterval(f,1); function show_olds() { document.body.innerHTML = "{"+olds.join("")+"}"; }; setTimeout(show_olds,15000)